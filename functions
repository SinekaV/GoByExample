package main

import "fmt"

// Function that takes two integers and returns their sum
func plus(a int, b int) int {
    return a + b
}

// Function with multiple consecutive parameters of the same type
func plusPlus(a, b, c int) int {
    return a + b + c
}

func main() {
    // Call the plus function
    res := plus(1, 2)
    fmt.Println("1+2 =", res) // Output: 1+2 = 3

    // Call the plusPlus function
    res = plusPlus(1, 2, 3)
    fmt.Println("1+2+3 =", res) // Output: 1+2+3 = 6
}
