package main

import "fmt"

// Function returning two integers
func vals() (int, int) {
    return 3, 7
}

func main() {
    // Using multiple return values with assignment
    a, b := vals()
    fmt.Println(a) // Output: 3
    fmt.Println(b) // Output: 7

    // Using blank identifier to ignore one value
    _, c := vals()
    fmt.Println(c) // Output: 7
}
