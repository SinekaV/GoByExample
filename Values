package main
import "fmt"

func main() {
    // Strings can be concatenated using +
    fmt.Println("go" + "lang")

    // Integer and float operations
    fmt.Println("1+1 =", 1+1)
    fmt.Println("7.0/3.0 =", 7.0/3.0)

    // Boolean operations
    fmt.Println(true && false)
    fmt.Println(true || false)
    fmt.Println(!true)
}

/*
Output:
golang
1+1 = 2
7.0/3.0 = 2.3333333333333335
false
true
false
*/
